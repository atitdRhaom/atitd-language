// first letter
// d = Bestimmter Artikel (der, die, das)
// t = Demonstrativpronomen
// a = Unbestimmter Artikel (kein plural)
// k = Negativartikel (Negation)

// second letter
// s = Singular
// p = Plural

// third letter
// n = Nominativ: wer oder was?
// g = Genitiv: wessen?
// d = Dativ: wem?
// a = Akkusativ: wen oder was?

// Entity masculine, feminine, neuter, masculinecollective, femininecollective, neutercollective;


// adjective describing traits or qualities associated with men or a masculine presentation
Translate masculine to German
{
  dsn "der"; dsg "des"; dsd "dem"; dsa "den";
  dpn "die"; dpg "der"; dpd "den"; dpa "die";

  tsn "dieser"; tsg "dieses"; tsd "diesem"; tsa "diesen";
  tpn "diese"; tpg "dieser"; tpd "diesen"; tpa "diese";

  asn "ein"; asg "eines"; asd "einem"; asa "einen";

  ksn "kein"; ksg "keines"; ksd "keinem"; ksa "keinen";
  kpn "keine"; kpg "keiner"; kpd "keinen"; kpa "keine";
}

// adjective describing traits or qualities associated with female or a feminine presentation
Translate feminine to German
{
  dsn "die"; dsg "der"; dsd "der"; dsa "die";
  dpn "die"; dpg "der"; dpd "den"; dpa "die";

  tsn "diese"; tsg "dieser"; tsd "dieser"; tsa "diese";
  tpn "diese"; tpg "dieser"; tpd "diesen"; tpa "diese";

  asn "eine"; asg "einer"; asd "einer"; asa "eine";

  ksn "keine"; ksg "keiner"; ksd "keiner"; ksa "keine";
  kpn "keine"; kpg "keiner"; kpd "keinen"; kpa "keine";
}


  dsn "das"; dsg "des"; dsd "dem"; dsa "das";
  dpn "die"; dpg "der"; dpd "den"; dpa "die";

  tsn "dieses"; tsg "dieses"; tsd "diesem"; tsa "dieses";
  tpn "diese"; tpg "dieser"; tpd "diesen"; tpa "diese";

  asn "ein"; asg "eines"; asd "einem"; asa "ein";

  ksn "kein"; ksg "keines"; ksd "keinem"; ksa "kein";
  kpn "keine"; kpg "keiner"; kpd "keinen"; kpa "keine";
}

